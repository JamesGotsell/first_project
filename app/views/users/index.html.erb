<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>     

<table>
  <thead>
    <tr>
      <th>name</th>
      <th>profile picture</th>
      <th>songs</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= image_tag user.picture%></td>
       <td><%= user.songs %></td>
      <td><%= link_to 'Show', user %></td>
    </tr>

    <% end %>
  </tbody>
</table>


<br>
